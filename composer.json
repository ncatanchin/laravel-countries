{
    "name": "ncatanchin/laravel-countries",
    "description": "Laravel Countries is a bundle for Laravel, providing Almost ISO 3166_2, 3166_3, currency, Capital and more for all countries. Bug kudos to Christoph Kempen, on whose work this package is heavily based (webpatser/laravel-countries).",
    "keywords": [
        "laravel",
        "countries",
        "iso_3166_2",
        "iso_3166_3"
    ],
    "homepage": "https://github.com/ncatanchin/laravel-countries",
    "license": "MIT",
    "require": {
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0",
        "illuminate/database": "^5.5|^6.0|^7.0|^8.0",
        "php": ">=7.0.0"
    },
    "autoload": {
        "psr-4": {
            "Catanchin\\Countries\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "source": "https://github.com/ncatanchin/laravel-countries",
        "issues": "https://github.com/ncatanchin/laravel-countries/issues"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Catanchin\\Countries\\CountriesServiceProvider"
            ]
        }
    }
}
